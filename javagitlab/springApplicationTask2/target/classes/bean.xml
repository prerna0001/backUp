<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="actr" class="com.stackroute.Actor">
        <property name="name" value="Pikku"></property>
        <property name="age" value="41"></property>
        <property name="gender" value="F"></property>
    </bean>
    
    <bean id="movi" class="com.stackroute.Movie">
        <property name="actor" ref="actr"></property>
    </bean>
</beans>